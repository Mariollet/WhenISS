  "version": "0.2.0",
  "configurations": [

    {
      "name": "Run",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "Run (web-renderer html)",
      "request": "launch",
      "type": "dart",
      "args": [
        "-d",
        "chrome",
        "--web-renderer",
        "html",
      ]
    },
    {
      "name": "Run (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "Run (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}